 });\n\n      var second = 3;\n      var timmer = setInterval(function () {\n        second--;\n\n        if (second) {\n          toast.message = \"\\u5012\\u8BA1\\u65F6 \".concat(second, \" \\u79D2\");\n        } else {\n          clearInterval(timmer); // 关闭 toast\n\n          _Toast.clear();\n        }\n      }, 1000);\n    }\n  }\n};",{"version":3,"sources":["toast.vue"],"names":[],"mappings":";;AA0BA,OAAA,SAAA,MAAA,oBAAA;AACA,OAAA,SAAA,MAAA,oBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,OADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,SAAA,EAAA;AAFA,GAFA;AAMA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,OADA,EACA;AACA,aAAA,OAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AACA,aAAA,OAAA,CAAA;AACA;AACA,QAAA,IAAA,EAAA,IAFA;AAGA;AACA,QAAA,OAAA,EAAA,QAJA;AAKA;AACA,QAAA,QAAA,EAAA;AANA,OAAA;AAQA,KAbA;AAcA,IAAA,YAdA,0BAcA;AACA,aAAA,OAAA,CAAA,MAAA;AACA,KAhBA;AAiBA,IAAA,UAjBA,wBAiBA;AACA,aAAA,IAAA,CAAA,MAAA;AACA,KAnBA;AAoBA,IAAA,QApBA,sBAoBA;AACA,UAAA,KAAA,GAAA,OAAA,OAAA,CAAA;AACA;AACA,QAAA,QAAA,EAAA,CAFA;AAGA;AACA,QAAA,WAAA,EAAA,IAJA;AAKA,QAAA,WAAA,EAAA,SALA;AAMA,QAAA,OAAA,EAAA;AANA,OAAA,CAAA;;AASA,UAAA,MAAA,GAAA,CAAA;AACA,UAAA,MAAA,GAAA,WAAA,CAAA,YAAA;AACA,QAAA,MAAA;;AACA,YAAA,MAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,gCAAA,MAAA;AACA,SAFA,MAEA;AACA,UAAA,aAAA,CAAA,MAAA,CAAA,CADA,CAEA;;AACA,iBAAA,KAAA;AACA;AACA,OATA,EASA,IATA,CAAA;AAUA;AAzCA;AANA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <van-header\n      title=\"Toast 轻提示\"\n      left-arrow\n    ></van-header>\n    <div class=\"van-content\">\n      <h4 class=\"cell-title\">文字提示</h4>\n      <van-button @click=\"showToast('提示内容')\">文字提示</van-button>\n      <van-button @click=\"showToast('这是一条很长的提示内容，超出内容会自动换行的')\">长文字提示</van-button>\n\n      <h4 class=\"cell-title\">加载提示</h4>\n      <van-button @click=\"loadingToast\">加载提示</van-button>\n\n      <h4 class=\"cell-title\">成功/失败提示</h4>\n      <van-button @click=\"successToast\">成功提示</van-button>\n      <van-button @click=\"errorToast\">失败提示</van-button>\n\n      <h4 class=\"cell-title\">高级用法</h4>\n      <van-button @click=\"advToast\">高级用法</van-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Toast } from 'vant';\nimport vanHeader from '@components/header';\nimport vanButton from '@components/button';\n\nexport default {\n  name: 'Toast',\n  components: {\n    vanHeader,\n    vanButton\n  },\n  methods: {\n    showToast (options) {\n      Toast(options);\n    },\n    loadingToast () {\n      Toast.loading({\n        // 遮罩\n        mask: true,\n        // 加载信息\n        message: '加载中...',\n        // 多少毫秒自动关闭toast，0为不关闭toast\n        duration: 1300\n      });\n    },\n    successToast () {\n      Toast.success('成功文案');\n    },\n    errorToast () {\n      Toast.fail('失败文案');\n    },\n    advToast () {\n      const toast = Toast.loading({\n        // 持续展示 toast\n        duration: 0,\n        // 禁用背景点击\n        forbidClick: true,\n        loadingType: 'spinner',\n        message: '倒计时 3 秒'\n      });\n\n      let second = 3;\n      const timmer = setInterval(() => {\n        second--;\n        if (second) {\n          toast.message = `倒计时 ${second} 秒`;\n        } else {\n          clearInterval(timmer);\n          // 关闭 toast\n          Toast.clear();\n        }\n      }, 1000);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.van-button {\n  margin-left: 10px;\n}\n</style>\n"],"sourceRoot":"src/showcase"}]}