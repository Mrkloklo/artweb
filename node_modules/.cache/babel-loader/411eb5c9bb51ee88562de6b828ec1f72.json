rm=\"onConfirm\"\n    ></van-picker>\n\n    <van-picker\n      v-model=\"showPicker3\"\n      :data=\"cityData3\"\n      @confirm=\"onConfirm\"\n    ></van-picker>\n\n    <van-picker\n      v-model=\"disablePicker\"\n      :data=\"pickerData4\"\n      @confirm=\"onConfirm\"\n    ></van-picker>\n\n    <van-picker\n      v-model=\"showLoadingPicker\"\n      :data=\"cityData3\"\n      loading\n      @confirm=\"onConfirm\"\n    ></van-picker>\n  </div>\n</template>\n\n<script>\nimport vanHeader from '@components/header';\nimport vanPicker from '@components/picker';\nimport vanButton from '@components/button';\nimport cityData3 from '@public/lib/city.data-3.js';\n\nexport default {\n  name: 'Picker',\n  components: {\n    vanPicker,\n    vanHeader,\n    vanButton\n  },\n  data () {\n    return {\n      showPicker1: false,\n      showPicker2: false,\n      showPicker3: false,\n      disablePicker: false,\n      showTitle: false,\n      showLoadingPicker: false,\n      pickerData1: [{\n        text: 'saber',\n        id: 'saber'\n      }, {\n        text: '远坂凛',\n        id: '远坂凛'\n      }, {\n        text: '逢坂大河',\n        id: '逢坂大河'\n      }],\n      pickerData2: [{\n        text: '江苏',\n        id: '江苏',\n        children: [{\n          text: '苏州',\n          id: '苏州'\n        }, {\n          text: '南京',\n          id: '南京'\n        }, {\n          text: '连云港',\n          id: '连云港'\n        }, {\n          text: '徐州',\n          id: '徐州'\n        }]\n      }, {\n        text: '浙江',\n        id: '浙江',\n        children: [{\n          text: '温州',\n          id: '温州'\n        }, {\n          text: '绍兴',\n          id: '绍兴'\n        }, {\n          text: '宁波',\n          id: '宁波'\n        }]\n      }],\n      cityData3,\n      pickerData4: [{\n        text: 'saber',\n        id: 'saber'\n      }, {\n        text: '西宫哨子',\n        id: '西宫哨子',\n        disabled: true\n      }, {\n        text: '远坂凛',\n        id: '远坂凛'\n      }, {\n        text: '逢坂大河',\n        id: '逢坂大河'\n      }],\n      result: ''\n    };\n  },\n  methods: {\n    onConfirm (value) {\n      this.result = value;\n    }\n  }\n};\n</script>\n\n<style scoped>\n/deep/ .van-button {\n  margin-left: 10px;\n  margin-right: 10px;\n  margin-bottom: 10px;\n}\n.result {\n  margin-top: 30px;\n  padding-left: 10px;\n  padding-right: 10px;\n  text-align: center;\n}\n</style>\n"],"sourceRoot":"src/showcase"}]}