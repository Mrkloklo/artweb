>\n        <van-tab title=\"标签 1\">\n          <div class=\"tab-content\">内容 1</div>\n        </van-tab>\n        <van-tab\n          title=\"标签 2\"\n          disabled\n        >\n          <div class=\"tab-content\">内容 2</div>\n        </van-tab>\n        <van-tab title=\"标签 3\">\n          <div class=\"tab-content\">内容 3</div>\n        </van-tab>\n      </van-tabs>\n\n      <h4 class=\"cell-title\">样式风格 card</h4>\n      <van-tabs type=\"card\">\n        <van-tab title=\"标签 1\"></van-tab>\n        <van-tab title=\"标签 2\"></van-tab>\n        <van-tab title=\"标签 3\"></van-tab>\n      </van-tabs>\n\n      <h4 class=\"cell-title\">点击事件</h4>\n      <van-tabs @click=\"onClick\">\n        <van-tab title=\"标签 1\">\n          <div class=\"tab-content\">内容 1</div>\n        </van-tab>\n        <van-tab title=\"标签 2\">\n          <div class=\"tab-content\">内容 2</div>\n        </van-tab>\n      </van-tabs>\n\n      <h4 class=\"cell-title\">粘性布局</h4>\n      <van-tabs sticky>\n        <van-tab\n          v-for=\"index in 12\"\n          :title=\"'选项 ' + index\"\n          :key=\"index\"\n        >\n          <div class=\"tab-content\">内容 {{ index }}</div>\n        </van-tab>\n      </van-tabs>\n\n      <h4 class=\"cell-title\">自定义标签</h4>\n      <van-tabs>\n        <van-tab\n          v-for=\"index in 2\"\n          :key=\"index\"\n        >\n          <div slot=\"title\">\n            <van-icon name=\"more-o\" />选项 {{ index }}\n          </div>\n          <div class=\"tab-content\">内容 {{ index }}</div>\n        </van-tab>\n      </van-tabs>\n\n      <h4 class=\"cell-title\">切换动画</h4>\n      <van-tabs animated>\n        <van-tab\n          v-for=\"index in 12\"\n          :key=\"index\"\n          :title=\"'选项 ' + index\"\n        >\n          <div class=\"tab-content\">内容 {{ index }}</div>\n        </van-tab>\n      </van-tabs>\n\n      <h4 class=\"cell-title\">滑动切换</h4>\n      <van-tabs swipeable>\n        <van-tab\n          v-for=\"index in 12\"\n          :key=\"index\"\n          :title=\"'选项 ' + index\"\n        >\n          <div class=\"tab-content\">内容 {{ index }}</div>\n        </van-tab>\n      </van-tabs>\n    </div>\n  </div>\n</template>\n\n<script>\nimport vanHeader from '@components/header';\nimport vanIcon from '@components/icon';\nimport { Tab, Tabs, Toast } from 'vant';\n\nexport default {\n  name: 'Tab',\n  components: {\n    vanHeader,\n    'van-tab': Tab,\n    'van-tabs': Tabs,\n    vanIcon\n  },\n  data () {\n    return {\n      active1: 2,\n      active2: 0\n    };\n  },\n  methods: {\n    onClickDisabled (index, title) {\n      Toast(`${title} 已被禁用`);\n    },\n    onClick (index, title) {\n      Toast(`当前选中 ${title}`);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.tab-content {\n  height: 50px;\n  line-height: 50px;\n  padding: 10px;\n  background-color: #fff;\n}\n.van-tabs {\n  margin-bottom: 20px;\n}\n.van-icon {\n  margin-right: 5px;\n  vertical-align: -2px;\n}\n</style>\n"],"sourceRoot":"src/showcase"}]}