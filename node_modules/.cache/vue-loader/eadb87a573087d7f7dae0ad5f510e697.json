iner\">\n    <van-header\n      title=\"Switch 开关\"\n      left-arrow\n    ></van-header>\n    <div class=\"van-content\">\n      <h4 class=\"cell-title\">基础用法</h4>\n      <van-switch v-model=\"checked1\" />\n\n      <h4 class=\"cell-title\">禁用状态</h4>\n      <van-switch\n        v-model=\"checked2\"\n        disabled\n      />\n\n      <h4 class=\"cell-title\">加载状态</h4>\n      <van-switch\n        v-model=\"checked3\"\n        loading\n      />\n\n      <h4 class=\"cell-title\">自定义大小</h4>\n      <van-switch\n        v-model=\"checked4\"\n        size=\"24px\"\n      />\n\n      <h4 class=\"cell-title\">自定义颜色</h4>\n      <van-switch\n        v-model=\"checked5\"\n        active-color=\"#07c160\"\n        inactive-color=\"#f44\"\n      />\n\n      <h4 class=\"cell-title\">异步控制</h4>\n      <van-switch\n        :value=\"checked6\"\n        @input=\"onInput\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport vanHeader from '@components/header';\nimport vanSwitch from '@components/switch';\nimport { Dialog } from 'vant';\n\nexport default {\n  name: 'component_switch',\n  components: {\n    vanHeader,\n    vanSwitch\n  },\n  data () {\n    return {\n      checked1: true,\n      checked2: true,\n      checked3: true,\n      checked4: true,\n      checked5: true,\n      checked6: true\n    };\n  },\n  methods: {\n    onInput (checked) {\n      Dialog.confirm({\n        title: '提醒',\n        message: '是否切换开关？'\n      }).then(() => {\n        this.checked6 = checked;\n      }).catch(() => {\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.van-content {\n  padding: 0 10px;\n}\n</style>\n"]}]}