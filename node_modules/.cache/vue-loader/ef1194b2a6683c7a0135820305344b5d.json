heckbox>\n      <van-checkbox disabled>复选框</van-checkbox>\n\n      <h4 class=\"cell-title\">自定义颜色</h4>\n      <van-checkbox\n        v-model=\"checked\"\n        checked-color=\"#07c160\"\n      >自定义颜色</van-checkbox>\n\n      <h4 class=\"cell-title\">自定义图标</h4>\n      <van-checkbox v-model=\"checked\">\n        自定义图标\n        <img\n          slot=\"icon\"\n          slot-scope=\"props\"\n          :src=\"props.checked ? icon.active : icon.normal\"\n          style=\"height: 20px;\"\n        >\n      </van-checkbox>\n\n      <h4 class=\"cell-title\">复选框组</h4>\n      <van-checkbox-group v-model=\"result\">\n        <van-checkbox\n          v-for=\"(item, index) in list\"\n          :key=\"index\"\n          :name=\"item\"\n        >\n          复选框 {{ item }}\n        </van-checkbox>\n      </van-checkbox-group>\n\n      <h4 class=\"cell-title\">设置最大可选数</h4>\n      <van-checkbox-group\n        v-model=\"result\"\n        :max=\"2\"\n      >\n        <van-checkbox\n          v-for=\"(item, index) in list\"\n          :key=\"index\"\n          :name=\"item\"\n        >\n          复选框 {{ item }}\n        </van-checkbox>\n      </van-checkbox-group>\n\n      <h4 class=\"cell-title\">搭配单元格组件使用</h4>\n      <van-checkbox-group v-model=\"result\">\n        <van-cell-group>\n          <van-cell\n            v-for=\"(item, index) in list\"\n            clickable\n            :key=\"item\"\n            :title=\"`复选框 ${item}`\"\n            @click=\"toggle(index)\"\n          >\n            <van-checkbox\n              :name=\"item\"\n              ref=\"checkboxes\"\n              style=\"padding: 0;\"\n            />\n          </van-cell>\n        </van-cell-group>\n      </van-checkbox-group>\n    </div>\n  </div>\n</template>\n\n<script>\nimport vanHeader from '@components/header';\nimport vanCheckboxGroup from '@components/checkboxgroup';\nimport vanCheckbox from '@components/checkbox';\nimport vanCellGroup from '@components/cellgroup';\nimport vanCell from '@components/cell';\n\nexport default {\n  name: 'CheckBox',\n  components: {\n    vanHeader,\n    vanCheckboxGroup,\n    vanCheckbox,\n    vanCellGroup,\n    vanCell\n  },\n  data () {\n    return {\n      checked: true,\n      icon: {\n        active: 'https://img.yzcdn.cn/public_files/2017/10/13/793c77793db8641c4c325b7f25bf130d.png',\n        normal: 'https://img.yzcdn.cn/public_files/2017/10/13/c547715be149dd3faa817e4a948b40c4.png'\n      },\n      list: ['a', 'b', 'c'],\n      result: ['a', 'b']\n    };\n  },\n  methods: {\n    toggle (index) {\n      this.$refs.checkboxes[index].toggle();\n    }\n  }\n};\n</script>\n\n<style scoped>\n.van-content {\n  padding: 10px;\n}\n\n.van-checkbox {\n  padding: 0 10px 10px 10px;\n}\n</style>\n"]}]}