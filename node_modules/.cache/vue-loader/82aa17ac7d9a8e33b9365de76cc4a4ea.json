e;\n      this.showList = false;\n\n      if (this.isEdit) {\n        this.list = this.list.map(item => (item.id === info.id ? info : item));\n      } else {\n        this.list.push(info);\n      }\n      this.chosenContactId = info.id;\n    },\n\n    // 删除联系人\n    onDelete (info) {\n      this.showEdit = false;\n      this.list = this.list.filter(item => item.id !== info.id);\n      if (this.chosenContactId === info.id) {\n        this.chosenContactId = null;\n      }\n    }\n  }\n};\n</script>\n"]}]}