s=\"container\">\n    <van-header\n      title=\"Popup 弹出层\"\n      left-arrow\n    ></van-header>\n    <div class=\"van-content\">\n      <h4 class=\"cell-title\">基础用法</h4>\n      <van-button @click=\"onClickCenterPopup\">弹出 Popup</van-button>\n\n      <h4 class=\"cell-title\">弹出位置</h4>\n      <van-button @click=\"onClickBottomPopup\">底部弹出</van-button>\n      <van-button @click=\"onClickTopPopup\">顶部弹出</van-button>\n      <van-button @click=\"onClickRightPopup\">右侧弹出</van-button>\n    </div>\n\n    <van-popup\n      v-model=\"show\"\n      :position=\"position\"\n      :overlay=\"overlay\"\n    >\n      <div\n        v-if=\"position === ''\"\n        class=\"position-center\"\n      >内容</div>\n      <div\n        v-else-if=\"position === 'top'\"\n        class=\"position-top\"\n      >内容</div>\n      <div\n        v-else-if=\"position === 'bottom'\"\n        class=\"position-bottom\"\n      >内容</div>\n      <div\n        v-else\n        class=\"position-right\"\n      >\n        <van-button @click=\"closePopup\">关闭弹层</van-button>\n      </div>\n    </van-popup>\n  </div>\n</template>\n\n<script>\nimport vanHeader from '@components/header';\nimport vanPopup from '@components/popup';\nimport vanButton from '@components/button';\n\nexport default {\n  name: 'Popup',\n  components: {\n    vanPopup,\n    vanHeader,\n    vanButton\n  },\n  data () {\n    return {\n      show: false,\n      position: '',\n      overlay: true\n    };\n  },\n  methods: {\n    onClickCenterPopup () {\n      this.show = true;\n      this.position = '';\n    },\n    onClickBottomPopup () {\n      this.show = true;\n      this.position = 'bottom';\n\n      setTimeout(() => {\n        this.show = false;\n      }, 1000);\n    },\n    onClickTopPopup () {\n      this.show = true;\n      this.position = 'top';\n\n      setTimeout(() => {\n        this.show = false;\n      }, 1000);\n    },\n    onClickRightPopup () {\n      this.show = true;\n      this.position = 'right';\n    },\n    closePopup () {\n      this.show = false;\n    }\n  }\n};\n</script>\n\n<style scoped>\n.van-content {\n  padding-left: 10px;\n  padding-right: 10px;\n}\n.van-button {\n  margin-right: 10px;\n}\n.position-center {\n  width: 150px;\n  height: 50px;\n  padding: 10px;\n}\n.position-top,\n.position-bottom {\n  color: #fff;\n  border-radius: 0;\n  line-height: 20px;\n  background-color: rgba(0, 0, 0, 0.8);\n  padding: 20px;\n}\n.van-popup--right {\n  width: 70%;\n  height: 100%;\n}\n.position-right {\n  padding: 20px;\n}\n</style>\n"]}]}